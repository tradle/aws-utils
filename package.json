    "@tradle/aws-cloudwatch-client": "file:packages/aws-cloudwatch-client",
    "@tradle/aws-combo": "file:packages/aws-combo",
    "@tradle/aws-common-utils": "file:packages/aws-common-utils",
    "@tradle/aws-iam-client": "file:packages/aws-iam-client",
    "@tradle/aws-lambda-client": "file:packages/aws-lambda-client",
    "@tradle/aws-s3-client": "file:packages/aws-s3-client",
    "@tradle/aws-sns-client": "file:packages/aws-sns-client"
  },
  "private": true,
  "workspaces": [
    "packages/*"
