{
  "name": "@tradle/aws-utils",
  "version": "1.0.0",
  "description": "various helpers for the AWS SDK",
  "main": "lib/index.js",
  "scripts": {
    "test": "env NODE_ENV=test lerna run test",
    "test:ci": "env NODE_ENV=test lerna run test:ci",
    "tswatch": "tsc -b packages -w",
    "tsbuild": "tsc -v; lerna run tsc",
    "publish:local": "lerna publish --registry http://localhost:4873",
    "publish": "npm run tsbuild && npm test && lerna publish",
    "postinstall": "lerna bootstrap --ci=false",
    "postbootstrap": "npm run link",
    "generate_geodata": "./scripts/generate_geodata/cli.js"
  },
  "keywords": [],
  "author": "Mark Vayngrib <mvayngrib@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/ip": "^1.1.0",
    "@types/json-stable-stringify": "^1.0.34",
    "@types/lodash": "^4.14.180",
    "@types/lru-cache": "~4.1.1",
    "@types/node": "~10.12.18",
    "@types/pino": "~5.8.4",
    "@types/sinon": "^10.0.11",
    "@types/tape": "^4.13.2",
    "aws-sdk": "^2.1001.0",
    "haversine": "^1.1.1",
    "javascript-stringify": "^2.1.0",
    "lerna": "^4.0.0",
    "lru-cache": "~5.1.1",
    "markdown-it": "^12.3.2",
    "sinon": "~7.2.3",
    "tape": "^5.5.2",
    "ts-node": "~8.0.2",
    "tslint": "~5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.7",
    "world-countries": "^4.0.0"
  },
  "dependencies": {
    "@tradle/aws-client-factory": "file:packages/aws-client-factory",
