{
  "name": "@tradle/aws-utils",
  "version": "1.0.0",
  "description": "various helpers for the AWS SDK",
  "main": "lib/index.js",
  "scripts": {
    "test": "env NODE_ENV=test lerna run test",
    "test:ci": "env NODE_ENV=test lerna run test:ci",
    "tswatch": "tsc -b packages -w",
    "tsbuild": "tsc -v; lerna run tsc",
    "publish:local": "lerna publish --registry http://localhost:4873",
    "bootstrap": "lerna bootstrap",
    "postbootstrap": "npm run link",
    "link": "./scripts/link.sh",
    "postlink": "npm run dedupe",
    "dedupe": "./scripts/dedupe.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/json-stable-stringify": "~1.0.32",
    "@types/lodash": "~4.14.120",
    "@types/lru-cache": "~4.1.1",
    "@types/node": "~10.12.18",
    "@types/pino": "~5.8.4",
    "aws-sdk": ">=2.290.0 <3",
    "lerna": "^3.10.7",
    "lru-cache": "~5.1.1",
    "sinon": "~7.2.3",
    "tape": "~5.3.0",
    "ts-node": "~8.0.2",
    "tslint": "~5.12.1",
    "tslint-config-prettier": "~1.17.0",
    "typescript": "~3.3.3333"
  },
  "dependencies": {
    "@tradle/aws-client-factory": "file:packages/aws-client-factory",
    "@tradle/aws-cloudformation-client": "file:packages/aws-cloudformation-client",
